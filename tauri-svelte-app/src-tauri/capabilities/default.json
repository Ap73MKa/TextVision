{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-open",
    "fs:read-files",
    "fs:allow-create",
    "fs:allow-exists",
    "fs:allow-remove",
    "fs:allow-applocaldata-write",
    "fs:scope-applocaldata"
  ]
}
